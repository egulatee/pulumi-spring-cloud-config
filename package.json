{
  "name": "@egulatee/pulumi-spring-cloud-config",
  "version": "1.3.0",
  "description": "Pulumi Dynamic Provider for integrating Spring Cloud Config Server with infrastructure-as-code projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "prepare": "husky"
  },
  "keywords": [
    "pulumi",
    "dynamic-provider",
    "spring-cloud-config",
    "config-server",
    "infrastructure-as-code",
    "iac",
    "configuration-management",
    "spring",
    "cloud-config"
  ],
  "author": "Eric Gulatee",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/egulatee/pulumi-spring-cloud-config.git"
  },
  "bugs": {
    "url": "https://github.com/egulatee/pulumi-spring-cloud-config/issues"
  },
  "homepage": "https://github.com/egulatee/pulumi-spring-cloud-config#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@pulumi/pulumi": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@pulumi/pulumi": "^3.145.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/npm": "13.1.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/google-protobuf": "3.15.12",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "16.5.0",
    "google-protobuf": "4.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "nock": "^14.0.10",
    "prettier": "^3.4.2",
    "semantic-release": "25.0.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "8.46.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
