{
  "name": "@egulatee/pulumi-spring-cloud-config",
  "version": "0.1.0",
  "description": "Pulumi Dynamic Provider for integrating Spring Cloud Config Server with infrastructure-as-code projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "prepare": "husky"
  },
  "keywords": [
    "pulumi",
    "dynamic-provider",
    "spring-cloud-config",
    "config-server",
    "infrastructure-as-code",
    "iac",
    "configuration-management",
    "spring",
    "cloud-config"
  ],
  "author": "Eric Gulatee",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/egulatee/pulumi-spring-cloud-config.git"
  },
  "bugs": {
    "url": "https://github.com/egulatee/pulumi-spring-cloud-config/issues"
  },
  "homepage": "https://github.com/egulatee/pulumi-spring-cloud-config#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@pulumi/pulumi": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@pulumi/pulumi": "^3.145.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nock": "^13.5.6",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
